REM
REM IOA REG 14, IOB REG 15
REM SET REG 7 BIT 7 TO ENABLE OUTPUT ON IOB / CLEAR BIT TO ENALBE INPUT
REM SET REG 7 BIT 6 TO ENABLE OUTPUT ON IOA / CLEAR BIT TO ENABLE INPUT
REM

5 POKE $6002,$FF
6 POKE $6003,$FF
10 DAT=$6001
20 CTL=$6000
25 RST=7
30 LA=7
40 WR=6
45 RD=5
50 GOSUB 3000
100 REG=7:VAL=$FE:GOSUB 1000
110 REG=14:VAL=2:GOSUB 1000:REM LIGHTS UP PIN 20
999 END
1000 POKE CTL,RST
1010 POKE CTL,LA
1020 POKE DAT,REG
1030 POKE CTL,RST
1040 POKE CTL,WR
1050 POKE DAT,VAL
1060 POKE CTL,RST
1080 RETURN
2000 POKE CTL,RST
2010 POKE CTL,LA
2020 POKE DAT,REG
2030 POKE CTL,RST
2040 POKE CTL,RD
2045 POKE $6003,$00
2050 INP = PEEK(DAT)
2055 POKE $6003,$FF
2060 POKE CTL,RST
2070 RETURN
3000 POKE CTL,0
3010 POKE CTL,RST
3020 RETURN
